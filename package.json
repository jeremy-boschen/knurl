{
  "name": "knurl",
  "description": "Desktop HTTP client.",
  "private": true,
  "version": "0.1.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "yarn run test:fe && yarn run test:be",
    "test:fe": "node scripts/run-vitest-groups.mjs",
    "test:be": "cd src-tauri && cargo test",
    "test:e2e": "wdio run ./wdio.conf.ts",
    "build": "tsc && vite build && cd src-tauri && cargo build --release",
    "analyze": "cross-env ANALYZE=1 vite build",
    "typecheck": "tsc --noEmit",
    "check": "bash scripts/check-local.sh",
    "format": "biome format --write && cd src-tauri && cargo fmt",
    "lint": "biome lint && cd src-tauri && cargo clippy --all-targets -- -D warnings",
    "tauri": "tauri",
    "portal:package": "node scripts/portal-package.mjs",
    "generate:icons": "node scripts/generate-icons.mjs",
    "oauth-server": "node scripts/oauth-mock-server.mjs",
    "security": "CHECK_TYPES=0 CHECK_LINT=0 CHECK_FMT_RUST=0 CHECK_CLIPPY=0 CHECK_TEST_FE=0 CHECK_TEST_BE=0 CHECK_AUDIT=1 CHECK_DENY=1 CHECK_GITLEAKS=1 bash scripts/check-local.sh",
    "update": "yarn up && cd src-tauri && cargo update",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.19.0",
